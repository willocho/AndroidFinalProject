<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map_nav_graph"
    app:startDestination="@id/mapsFragment">

    <fragment
        android:id="@+id/mapsFragment"
        android:name="edu.utap.finalproject.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps"/>
    <fragment
        android:id="@+id/reservationFragment"
        android:name="edu.utap.finalproject.ReservationsFragment"
        android:label="Reservations"/>
    <fragment
        android:id="@+id/reserveFragment"
        android:name="edu.utap.finalproject.ReserveFragment"
        android:label="Reserve">
        <argument
            android:name="car"
            app:argType="edu.utap.finalproject.repository.car.CarLocalModel"
            app:nullable="true"
            />
    </fragment>
    <action
        android:id="@+id/action_to_reservations"
        app:destination="@+id/reservationFragment"
        />
    <action
        android:id="@+id/action_to_map"
        app:destination="@+id/mapsFragment"
        />
    <action
        android:id="@+id/action_to_reserve"
        app:destination="@id/reserveFragment"
        />
</navigation>